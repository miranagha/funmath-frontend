shape \nswitch(element.shape){case 1:// if the shape is triangle\nif(p5.mouseX<element.x_cord+50&&p5.mouseY<element.y_cord+50){if(ques_shape===element.shape){// check the ques shape\nvar index=shape.indexOf(element);if(index>-1){shape.splice(index,1);// remove frrom the display\n}game6_score++;change_ques=true;}else{game6_answer=false;}}else{}break;case 2:/// if the shape is half circle\nif(p5.mouseX<element.x_cord+50&&p5.mouseY<element.y_cord+50){if(ques_shape===element.shape){var _index=shape.indexOf(element);if(_index>-1){shape.splice(_index,1);}game6_score++;change_ques=true;}else{game6_answer=false;}}else{}break;case 3:// if the shape is square\nif(p5.mouseX<element.x_cord+50&&p5.mouseY<element.y_cord+50){if(ques_shape===element.shape){var _index2=shape.indexOf(element);if(_index2>-1){shape.splice(_index2,1);}game6_score++;change_ques=true;}else{game6_answer=false;}}else{}break;case 4:// if the shape is rectangle\nif(p5.mouseX<element.x_cord+80&&p5.mouseY<element.y_cord+50){if(ques_shape===element.shape){var _index3=shape.indexOf(element);if(_index3>-1){shape.splice(_index3,1);}game6_score++;change_ques=true;}else{game6_answer=false;}}else{}break;case 5:// if the shape is circle\nif(p5.mouseX<element.x_cord+50&&p5.mouseY<element.y_cord+50){if(ques_shape===element.shape){var _index4=shape.indexOf(element);if(_index4>-1){shape.splice(_index4,1);}game6_score++;change_ques=true;}else{game6_answer=false;}}else{}break;case 6:// if the shape is ellipse\nif(p5.mouseX<element.x_cord+50&&p5.mouseY<element.y_cord+50){if(ques_shape===element.shape){var _index5=shape.indexOf(element);if(_index5>-1){shape.splice(_index5,1);}game6_score++;change_ques=true;}else{game6_answer=false;}}else{}break;default:break;}}});}else if(!game6_answer){// if tapped on retry \nif(mouse_x>20&&mouse_y>20&&mouse_x<100&&mouse_y<40){game6_answer=true;dropped=false;game6_score=0;}}};return/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(build_default.a,{setup:setup,draw:draw,style:{justifyContent:'center',